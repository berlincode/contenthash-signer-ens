{
  "name": "contenthash-signer-ens",
  "version": "3.1.0",
  "description": "Sign EIP-1577 compatible contenthashes (e.g. IPFS CIDs) hashes and content version (in an ethereum compatible way)",
  "homepage": "https://github.com/berlincode/contenthash-signer-ens",
  "license": "MIT",
  "main": "js/index.js",
  "author": {
    "name": "Ulf Bartel",
    "email": "elastic.code@gmail.com"
  },
  "scripts": {
    "compile": "solcjs --optimize --bin --abi ENSResolver.sol",
    "compile-test-contract": "cd test/contracts && solcjs --optimize --bin --abi ENSRegistry.sol",
    "ethlint": "solium --file IpfsSigner.sol",
    "test": "mocha",
    "eslint": "eslint ./js/*.js ./test/*.js ./examples/*.js ./*.html",
    "build": "rm -rf dist && mkdir dist && cp *.html dist && cp -r js dist && cp -r assets dist && mkdir -p dist/node_modules/cids/dist/ && cp node_modules/cids/dist/index.js dist/node_modules/cids/dist/"
  },
  "files": [
    "ENSResolver.sol",
    "ENSResolver_sol_ResolverContenthashSignerENS.abi",
    "ENSResolver_sol_ResolverContenthashSignerENS.bin",
    "js/*",
    "examples/generate.js",
    "examples/validate.js",
    "LICENSE",
    "README"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/berlincode/contenthash-signer-ens.git"
  },
  "keywords": [
    "ens",
    "resolver",
    "sign",
    "signature",
    "smart contract",
    "ethereum"
  ],
  "dependencies": {
    "cids": "^0.5.7",
    "ethereumjs-util": "^6.0.0",
    "web3": "1.0.0-beta.37"
  },
  "devDependencies": {
    "eth-ens-namehash": "^2.0.8",
    "eslint": "5.10.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-ie11": "^1.0.0",
    "ganache-core": "^2.3.3",
    "gh-pages": "^2.0.1",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "solc": "0.5.3",
    "ethlint": "^1.2.2"
  }
}
